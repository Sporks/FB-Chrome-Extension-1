{
   "manifest_version": 2,

   "name": "Star Wars Theme",
   "version": "0.01",

   "browser_action": {
      "default_icon": {
         "19": "icon-19.png",
         "38": "icon-38.png"
      },
      "tooltip": "StarWars Mode!"
   },
   "permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
               "http://*/*",
               "https://*/*"
           ],
			"js": ["bower_components/underscore/underscore-min.js", "bower_components/jquery/dist/jquery.min.js", "action.js"]

		}
	],
	"background": {
		"scripts": ["action.js"],
		"persistent": false
	}
}